{{ form_start(form) }}
<div class="row adresses">
    <div class="col-md-1 col-lg-1">
        <div class="form-group">
            {{ form_row(form.number) }}
            {{ form_errors(form.number) }}
        </div>
    </div>
    <div class="col-md-3 col-lg-3">
        <div class="form-group">
            {{ form_row(form.street) }}
            {{ form_errors(form.street) }}
            {#<input type="text" id="street" name="street" value="{{ data.street }}"
                   class="form-control"  onkeyup="this.value = this.value.toUpperCase();"/>#}
        </div>
    </div>
    <div class="col-md-3 col-lg-2">
        <div class="form-group">
            {{ form_row(form.postalCode) }}
            {{ form_errors(form.postalCode) }}
        </div>
    </div>
    <div class="col-md-2 col-lg-3">
        <div class="form-group">
            {{ form_row(form.city) }}
            {{ form_errors(form.city) }}
            {#<input type="text" id="city" name="city" value="{{ data.city }}" class="form-control" onkeyup="this.value = this.value.toUpperCase();"/>#}
        </div>
    </div>

    <div class="col-md-3 col-lg-3">
        <div class="form-group">
            {{ form_row(form.country) }}
            {{ form_errors(form.country) }}
        </div>
    </div>


</div>


<div class="row">
    <div class="col-lg-12">
        <div class="pull-right">
            <button class="btn btn-success btn-lg" type="submit">Envoyer</button>
        </div>
    </div>
</div>
{{ form_end(form) }}

